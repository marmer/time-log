{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "encoding": true,
        "interface-name": false,
        "max-line-length": false,
        "arrow-parens": false,
        "no-console": false,
        "object-literal-sort-keys": {
            "options": [
                "match-declaration-order"
            ]
        }
    },
    "rulesDirectory": []
}
